				<xs:annotation>
					<xs:documentation>
						Describes the type of withingholding when WithholdingType is set to Other.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			
			<xs:element name="withholdingFilingStatus" type="WithholdingFilingStatus"  minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>
						Describes the withholding filing status to be applied for withholdings calculation. 
					</xs:documentation>
				</xs:annotation>
			</xs:element>

			<xs:element name="numberOfWithholdings" type="xs:string"  minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>
						The number of withholdings or allowances declared. Example form W-4 line 5. 
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			
			<xs:element name="withholdingsAmount" type="Money"  minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>
						Additional amount of withholdings to be withheld from a payment such as a paycheck. Example W-4 line 6
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			
			<xs:element name="withholdingsExempt" type="xs:boolean"  minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>
						Withholdings are exempt when set to True.
					</xs:documentation>
				</xs:annotation>
			</xs:element>

			<xs:element name="withholdingsRate" type="xs:decimal"  minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>
						The rate of withholding to apply to a qualifying transaction. 
						Value is formatted as a percentile. A passed value of 4.5 is equivalent to 0.045 actual.
						4.500 may be displayed as 4.500% or 4.5% depending on the user interface.
						Decimals available: 999.999 
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			
			<xs:element name="withholdingsOption" type="xs:string"  minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>
						Describes the withholding option as defined by the financial institution 
						and allows for the range of special instructions that may be applicable.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
			
	<xs:complexType name="WithholdingsInfoList">
		<xs:annotation>
			<xs:documentation>
				The list of withholding information.
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="ListBase">
				<xs:sequence>
					<xs:element name="withholdingInfo" type="WithholdingInfo" minOccurs ="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>
								Fields related to require tax withholding information.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
			
	<xs:simpleType name="AchPrenoteIndicator">
		<xs:restriction base="xs:boolean">
			<xs:annotation>
				<xs:documentation>
					ACH Prenote Indicator is set to True when an alert of the transfer will be sent to the receiving institution
					in advance of the actual transfer. 
				</xs:documentation>
			</xs:annotation>
		</xs:restriction>
	</xs:simpleType>
	
	<xs:simpleType name="UserId">
		<xs:restriction base="xs:string">
			<xs:annotation>
				<xs:documentation>
					The unique identifier created by the system and or the user for identification.
				</xs:documentation>
			</xs:annotation>
		</xs:restriction>
	</xs:simpleType>
	
	<xs:simpleType name="IncludeNotesFlag">
		<xs:restriction base="xs:boolean">
			<xs:annotation>
				<xs:documentation>
					If this is not provided, it will not return notes.
					If it is provided and it's true, it will return all active non-expired notes.
				</xs:documentation>
			</xs:annotation>
		</xs:restriction>
	</xs:simpleType>
	
	<xs:simpleType name ="PartyId">
		<xs:annotation>
			<xs:documentation>
				The party ID is a persistent and unique identifier for the party not related to the accounts
				they own but tied to that person or organization. It should be unique for all parties across the
				institution and remain the same over the life of the relationship with the financial institution.
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string" ></xs:restriction>
	</xs:simpleType>

	<xs:complexType name="PartyIdList">
		<xs:annotation>
			<xs:documentation>
				The list of party ID's that should be returned or affected by a read, update or delete request.
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="ListBase">
				<xs:sequence>
					<xs:element name="partyId" type="PartyId" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>
								A party ID that should have its associated parties either returned or affected
								by a read, update or delete request.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
</xs:schema>

