  <xs:simpleType name="ConfigurationFinancialInstitutionAppId">
    <xs:annotation>
      <xs:documentation>
        The unique identifier for the configuration, financial institution, app and parameter combination.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
    </xs:restriction>
  </xs:simpleType>
  
  <xs:complexType name="ConfigurationFinancialInstitutionAppIdList">
    <xs:annotation>
      <xs:documentation>
        The list of Configuration Fi App ID's that should have their associated configurations either returned or affected
        by a read, update or delete configuration request.
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="common:ListBase">
        <xs:sequence>
          <xs:element name="configurationFiAppId" type="ConfigurationFinancialInstitutionAppId" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
